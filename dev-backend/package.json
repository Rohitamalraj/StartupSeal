{
  "name": "trust-oracle-ai-backend",
  "version": "1.0.0",
  "description": "AI-powered trust scoring engine with Nautilus compute integration",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "train": "python ai/pipelines/train_model.py",
    "setup": "npm install && pip install -r ai/requirements.txt && echo Setup complete!",
    "docker:build": "docker build -t trust-oracle-ai .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "ai",
    "nautilus",
    "walrus",
    "trust-scoring",
    "blockchain",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "mongoose": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "multer": "^1.4.5-lts.2",
    "cheerio": "^1.0.0-rc.12",
    "node-cron": "^3.0.3",
    "@octokit/rest": "^20.0.2",
    "form-data": "^4.0.0",
    "exif-parser": "^0.1.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
