# Server Configuration
PORT=5000
NODE_ENV=development

# Walrus Decentralized Storage (REQUIRED)
WALRUS_API_URL=https://aggregator.walrus-testnet.walrus.space
WALRUS_STORAGE_ENDPOINT=https://publisher.walrus-testnet.walrus.space

# Nautilus Package (Sui Blockchain)
NAUTILUS_PACKAGE_ID=your_nautilus_package_id

# Sui Blockchain Configuration (REQUIRED for StartupSeal NFT fetching)
SUI_PACKAGE_ID=your_sui_package_id
SEAL_REGISTRY=your_seal_registry_shared_object_id

# Google Vision API (REQUIRED for AI Document Legitimacy Check)
# Get your API key from: https://console.cloud.google.com/apis/credentials
GOOGLE_VISION_API_KEY=your_google_vision_api_key

# GitHub OAuth (REQUIRED for repository verification)
# Create OAuth app at: https://github.com/settings/developers
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Optional APIs
GITHUB_TOKEN=your_github_personal_access_token
